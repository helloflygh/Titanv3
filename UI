    <div id="panic-overlay"><span style="position:absolute; top:15px; left:15px; font-size:30px; cursor:pointer;" onclick="closePanic()">&times;</span><iframe id="panic-iframe"></iframe></div>

    <script>
        const quotes = ["Who actually reads these?", "50+ Games!", "Mateooooooooooooooooooooooooo", "Don't get lost in 'The Odyssey'. T!tans here for YOU!", "R.I.P. n3bula and s4turn.", "Major Updates every Monday!!!", "TYSM S4turn <3", "T!tans way - Games for when teachers look away", "Is this update even good?", "I think mavrick had a lil too much of his mothers crystals", "Keep clicking, I dare you.", "The best unblocked site... probably."];
        let lastQuoteIndex = -1;

        function initSite() {
            document.getElementById("panicUrlInput").value = localStorage.getItem('panicLink') || 'https://clever.com';
            document.getElementById("panicTabToggle").checked = localStorage.getItem('panicTabEnabled') === 'true';
            document.getElementById("shortcutToggle").checked = localStorage.getItem('panicShortcutEnabled') !== 'false';
            document.getElementById("panicKeyInput").value = localStorage.getItem('panicKey') || 'x';
            updateQuote();
            setInterval(updateQuote, 8000);
        }

        function updateQuote() {
            const display = document.getElementById("quoteDisplay");
            let newIndex;
            do { newIndex = Math.floor(Math.random() * quotes.length); } while (newIndex === lastQuoteIndex);
            lastQuoteIndex = newIndex;
            display.style.opacity = 0;
            setTimeout(() => {
                display.textContent = quotes[newIndex];
                display.style.opacity = 1;
            }, 500);
        }

        function handleNav(isOpen) { document.getElementById("navPanel").style.width = isOpen ? "250px" : "0"; }
        function toggleSettings(show) {
            document.getElementById("mainLinks").style.display = show ? "none" : "block";
            document.getElementById("settingsMenu").style.display = show ? "block" : "none";
        }

        function changeContent(id) { 
            document.querySelectorAll('.page-content').forEach(p => p.classList.remove('active-page')); 
            document.getElementById(id).classList.add('active-page'); 
            handleNav(false); 
        }

        function doPanic() {
            let url = document.getElementById("panicUrlInput").value.trim() || 'https://clever.com';
            if (document.getElementById("panicTabToggle").checked) {
                window.open(url.startsWith('http') ? url : 'https://' + url, '_blank');
            } else {
                document.getElementById("panic-iframe").src = url.startsWith('http') ? url : 'https://' + url;
                document.getElementById("panic-overlay").style.display = "block";
            }
        }

        function closePanic() { document.getElementById("panic-overlay").style.display = "none"; }
        function saveSettings() {
            localStorage.setItem('panicLink', document.getElementById("panicUrlInput").value);
            localStorage.setItem('panicTabEnabled', document.getElementById("panicTabToggle").checked);
            localStorage.setItem('panicShortcutEnabled', document.getElementById("shortcutToggle").checked);
            localStorage.setItem('panicKey', document.getElementById("panicKeyInput").value.toLowerCase());
        }

        function filterImages() {
            let q = document.getElementById("search").value.toLowerCase();
            let matches = 0;
            document.querySelectorAll(".item").forEach(i => {
                let match = i.dataset.title.toLowerCase().includes(q);
                i.style.display = match ? "block" : "none";
                if(match) matches++;
            });
            document.getElementById("no-results").style.display = matches === 0 ? "block" : "none";
        }

        window.addEventListener('keydown', (e) => {
            const isEnabled = document.getElementById("shortcutToggle").checked;
            const targetKey = (document.getElementById("panicKeyInput").value || 'x').toLowerCase();
            if (isEnabled && e.ctrlKey && e.key.toLowerCase() === targetKey) {
                e.preventDefault(); doPanic();
            }
        });

        function openInBlank() {
            let win = window.open('about:blank', '_blank');
            if (win) {
                win.document.write(`<html><head><title>about:blank</title><style>body,html{margin:0;padding:0;height:100%;overflow:hidden;}</style></head><body>${document.documentElement.outerHTML}</body></html>`);
                win.document.close();
            }
        }

        function launchProxy() {
            const input = document.getElementById('proxyInput').value;
            const domain = "https://realworld.helpforukraine.at/render.html?url="; 
            const encoded = btoa(input);
            const fullUrl = domain + encoded;
            const newWin = window.open('about:blank', '_blank');
            if (newWin) {
                newWin.document.write(`
                    <!DOCTYPE html>
                    <html>
                    <head>
                        <title>Clever | Portal</title>
                        <style>
                            body, html { margin: 0; padding: 0; height: 100%; overflow: hidden; background: #000; }
                            #top-bar { width: 100%; display: flex; justify-content: center; position: absolute; top: 20px; left: 0; z-index: 9999; pointer-events: none; }
                            .panic-btn { pointer-events: auto; font-size: 18px; padding: 10px 40px; color: #000; background: #ff3333; font-family: 'Arial Black', sans-serif; cursor: pointer; border-radius: 8px; font-weight: 900; border: 3px solid #000; box-shadow: 0 4px 10px rgba(0,0,0,0.5); text-transform: uppercase; transition: transform 0.1s; }
                            #main-game-iframe { width: 100%; height: 100%; border: none; }
                            #panic-overlay { display: none; position: fixed; top: 0; left: 0; width: 100vw; height: 100vh; background: #fff; z-index: 1000000; }
                            #panic-iframe { width: 100%; height: 100%; border: none; }
                            .close-panic { position: absolute; top: 15px; right: 20px; cursor: pointer; font-size: 24px; font-family: Arial, sans-serif; color: #ccc; font-weight: bold; z-index: 1000001; user-select: none; }
                        </style>
                    </head>
                    <body>
                        <div id="top-bar"><button class="panic-btn" onclick="doPanic()">PANIC</button></div>
                        <iframe id="main-game-iframe" src="${fullUrl}"></iframe>
                        <div id="panic-overlay"><span class="close-panic" onclick="closePanic()">√ó</span><iframe id="panic-iframe"></iframe></div>
                        <script>
                            const PANIC_URL = "https://clever.com"; 
                            function doPanic() { document.getElementById("panic-iframe").src = PANIC_URL; document.getElementById("panic-overlay").style.display = "block"; }
                            function closePanic() { document.getElementById("panic-overlay").style.display = "none"; document.getElementById("panic-iframe").src = ""; }
                            window.addEventListener('keydown', (e) => { if (e.ctrlKey && e.key.toLowerCase() === 'x') { doPanic(); } });
                        <\/script>
                    </body>
                    </html>
                `);
                newWin.document.close();
            } else {
                alert("Pop-up blocked! Please allow pop-ups.");
            }
        }

        /* --- NEW: Game Injection Launcher --- */
        function launchGame(gameUrl, gameTitle) {
            const panicUrl = document.getElementById("panicUrlInput").value || 'https://clever.com';
            const newWin = window.open('about:blank', '_blank');
            
            if (newWin) {
                const loaderHtml = `
                    <!DOCTYPE html>
                    <html lang="en">
                    <head>
                        <meta charset="UTF-8">
                        <title>Clever | Portal</title>
                        <style>
                            html, body { margin: 0; width: 100%; height: 100%; overflow: hidden; background: #000; }
                            #viewer { width: 100%; height: 100%; border: none; background: white; }
                            .panic-btn {
                                position: fixed; top: 20px; left: 50%; transform: translateX(-50%);
                                font-size: 24px; padding: 18px 50px; color: #000 !important; background: #ff3333;
                                font-family: 'Arial Black', sans-serif; cursor: pointer; z-index: 99999;
                                border-radius: 12px; font-weight: 900; border: 4px solid #000;
                                box-shadow: 0 6px 20px rgba(0,0,0,0.5); transition: all 0.2s ease-in-out;
                                text-transform: uppercase; letter-spacing: 3px;
                            }
                            #panic-overlay { display: none; position: fixed; top: 0; left: 0; width: 100vw; height: 100vh; background: #fff; z-index: 100000; }
                            #panic-iframe { width: 100%; height: 100%; border: none; }
                            .close-panic { position: absolute; top: 15px; left: 15px; font-size: 35px; cursor: pointer; color: #000 !important; font-weight: bold; z-index: 100001; }
                        </style>
                    </head>
                    <body>
                        <button class="panic-btn" onclick="doPanic()">PANIC</button>
                        <iframe id="viewer"></iframe>
                        <div id="panic-overlay">
                            <span class="close-panic" onclick="closePanic()">&times;</span>
                            <iframe id="panic-iframe"></iframe>
                        </div>
                        <script>
                            const githubHTMLUrl = "${gameUrl}";
                            const PANIC_URL = "${panicUrl}"; 
                            async function loadGame() {
                                try {
                                    const response = await fetch(githubHTMLUrl);
                                    let html = await response.text();
                                    const baseUrl = githubHTMLUrl.substring(0, githubHTMLUrl.lastIndexOf("/") + 1);
                                    html = html.replace("<head>", '<head><base href="' + baseUrl + '">');
                                    const iframe = document.getElementById("viewer");
                                    const iframeDoc = iframe.contentDocument || iframe.contentWindow.document;
                                    iframeDoc.open();
                                    iframeDoc.write(html);
                                    iframeDoc.close();
                                    iframe.onload = function() {
                                        try {
                                            iframe.contentWindow.addEventListener('keydown', (e) => {
                                                if (e.ctrlKey && e.key.toLowerCase() === 'x') { e.preventDefault(); doPanic(); }
                                            });
                                        } catch (e) { console.log("Hotkey injection limited."); }
                                    };
                                } catch (err) { console.error(err); }
                            }
                            function doPanic() {
                                document.getElementById("panic-iframe").src = PANIC_URL;
                                document.getElementById("panic-overlay").style.display = "block";
                            }
                            function closePanic() {
                                document.getElementById("panic-overlay").style.display = "none";
                                document.getElementById("panic-iframe").src = "";
                            }
                            window.addEventListener('keydown', (e) => {
                                if (e.ctrlKey && e.key.toLowerCase() === 'x') { e.preventDefault(); doPanic(); }
                            });
                            loadGame();
                        <\/script>
                    </body>
                    </html>
                `;
                newWin.document.write(loaderHtml);
                newWin.document.close();
            } else {
                alert("Pop-up blocked! Please allow pop-ups to launch games.");
            }
        }
    </script>
</body>
</html>
